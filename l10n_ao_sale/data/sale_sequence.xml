<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">

        <record id="l10n_ao_sale_qt" model="ir.sequence">
            <field name="name">Quotation</field>
            <field name="prefix">%(range_year)s/</field>
            <field name="code">OU</field>
            <field name="company_id" eval="False"/>
            <field eval="0" name="padding"/>
            <field name="use_date_range">True</field>
            <field name="implementation">no_gap</field>
        </record>

        <record id="l10n_ao_sale_or" model="ir.sequence">
            <field name="name">Proposal</field>
            <field name="prefix">%(range_year)s/</field>
            <field name="code">OR</field>
            <field name="company_id" eval="False"/>
            <field name="padding" eval="0"/>
            <field name="use_date_range">True</field>
            <field name="implementation">no_gap</field>
        </record>

        <record id="l10n_ao_sale_pp" model="ir.sequence">
            <field name="name">Pro Forma</field>
            <field name="prefix">%(range_year)s/</field>
            <field name="code">PP</field>
            <field name="company_id" eval="False"/>
            <field eval="0" name="padding"/>
            <field name="use_date_range">True</field>
            <field name="implementation">no_gap</field>
        </record>

    </data>
</odoo>